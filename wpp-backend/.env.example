# Configurações do Servidor WPPConnect
PORT=3333
NODE_ENV=development

# Configurações do WhatsApp (opcionais)
WPP_SESSION_NAME=crm-session
WPP_HEADLESS=true
WPP_DEBUG=false

# CORS Origins (separados por vírgula)
CORS_ORIGINS=http://localhost:5173,https://sistema-crm-imobili-9zrh.bolt.host

# Configurações de Upload
MAX_FILE_SIZE=16777216
UPLOAD_DIR=./uploads

# Configurações de Rate Limiting (opcional)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100