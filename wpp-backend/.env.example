# Configurações do Servidor WPPConnect
PORT=3333
NODE_ENV=development

# Redis para storage de sessões (opcional)
REDIS_URL=redis://localhost:6379
# ou para Redis Cloud: redis://user:password@host:port

# Configurações do WhatsApp (opcionais)
SESSION_NAME=crm-pro
WPP_HEADLESS=true
WPP_DEBUG=false

# Frontend Origin para CORS
FRONTEND_ORIGIN=https://seu-frontend.bolt.host

# Configurações de Upload
MAX_FILE_SIZE=16777216